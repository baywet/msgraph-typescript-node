{
  "name": "msgraph-typescript-node",
  "version": "1.0.0",
  "description": "Base repository for all Microsoft Graph TypeScript + Node tests",
  "main": "index.js",
  "dependencies": {
    "@azure/identity": "^3.3.0",
    "@microsoft/kiota-authentication-azure": "^1.0.0-preview.28",
    "@microsoft/msgraph-sdk-javascript": "^1.0.0-preview.7",
    "hpagent": "^1.2.0",
    "dotenv": "^16.0.1"
  },
  "scripts": {
    "proxy": "m365proxy -a 429 -f 50",
    "start": "npm run build:dev && node './dist/bundle.js'",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",    
    "start:proxy": "cross-env DEBUG=true HTTP_PROXY=http://127.0.0.1:8000 HTTPS_PROXY=http://127.0.0.1:8000 npm-run-all --parallel proxy start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastienlevert/msgraph-typescript-node.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastienlevert/msgraph-typescript-node/issues"
  },
  "homepage": "https://github.com/sebastienlevert/msgraph-typescript-node#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "dotenv-webpack": "^8.0.0",
    "ts-loader": "^9.3.1"
  }
