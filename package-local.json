{
  "name": "msgraph-typescript-node",
  "version": "1.0.0",
  "description": "Base repository for all Microsoft Graph TypeScript + Node tests",
  "main": "index.js",
  "sideEffects": false,
  "dependencies": {
    "@azure/identity": "^3.4.1",
    "@azure/identity-cache-persistence": "^1.0.0",
    "@microsoft/kiota-abstractions": "file:../../microsoft/kiota-typescript/packages/abstractions",
    "@microsoft/kiota-authentication-azure": "file:../../microsoft/kiota-typescript/packages/authentication/azure",
    "@microsoft/kiota-http-fetchlibrary": "file:../../microsoft/kiota-typescript/packages/http/fetch",
    "@microsoft/kiota-serialization-form": "file:../../microsoft/kiota-typescript/packages/serialization/form",
    "@microsoft/kiota-serialization-json": "file:../../microsoft/kiota-typescript/packages/serialization/json",
    "@microsoft/kiota-serialization-multipart": "file:../../microsoft/kiota-typescript/packages/serialization/multipart",
    "@microsoft/kiota-serialization-text": "file:../../microsoft/kiota-typescript/packages/serialization/text",
    "dotenv": "^16.0.1",
    "guid-typescript": "^1.0.9",
    "hpagent": "^1.2.0"
  },
  "scripts": {
    "clean:generated": "rimraf ./src/generated",
    "clean:dist": "rimraf ./dist",
    "clean:kiota": "rimraf ./.kiota",
    "clean": "rimraf ./node_modules && npm run clean:dist && npm run clean:generated && npm run clean:kiota && rimraf ./package-lock.json",
    "kiota": "npm run kiota:install && npm run kiota:override && npm run kiota:generate",
    "kiota:install": "kiota --version",
    "kiota:override": "cp /home/slevert/src/microsoft/kiota/src/kiota/bin/Release/net8.0/linux-x64/publish/kiota .kiota/v1.9.1/kiota",
    "kiota:generate": "kiota generate -l typescript -d https://aka.ms/graph/v1.0/openapi.yaml -i '/appCatalogs' -i '/appCatalogs/**' -e '/me/**' -e '/me' -c GraphClient -o ./src/generated",
    "proxy": "devproxy -a 429 -f 50",
    "start": "npx ts-node ./src/index.ts",
    "start:dev": "npm run build:dev && node './dist/bundle.js'",
    "start:prod": "npm run build:prod && node './dist/bundle.js'",
    "build:dev": "npm run clean:dist && webpack --config webpack.dev.js --progress",
    "build:prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 && npm run clean:dist && webpack --config webpack.prod.js --progress",
    "rebuild:dev": "npm run clean && npm install && npm run kiota && npm run build:dev",
    "rebuild:prod": "npm run clean && npm install && npm run kiota && npm run build:prod",
    "start:proxy": "cross-env DEBUG=true HTTP_PROXY=http://127.0.0.1:8000 HTTPS_PROXY=http://127.0.0.1:8000 npm-run-all start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastienlevert/msgraph-typescript-node.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastienlevert/msgraph-typescript-node/issues"
  },
  "homepage": "https://github.com/sebastienlevert/msgraph-typescript-node#readme",
  "devDependencies": {
    "@kiota-community/kiota-gen": "^1.0.0",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.0",
    "node-loader": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "shx": "^0.3.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
